package name;
import java.util.Scanner;
public class Array_highest_element1D {

	public static void main(String[] args) {
	Scanner s=new Scanner(System.in);
		
	    int size,temp=0;                          // for size
	    
		System.out.println("enter the size");
		size=s.nextInt();
	
		int a[]=new int[size];
		
		System.out.println("enter the elements");
		for(int i=0;i<a.length;i++)       //user input
		{
			a[i]=s.nextInt();
		}
		int max=a[0];
		
		
		for(int i=0;i<a.length;i++)      //swap
		{  
			if(max<a[i])
				{ 
				temp=max;				
				max=a[i];
				}
			}
		
		
		System.out.println("the 2nd highest element is: "+temp);
		
	}
}
